{
    "version": 2,
    "cleanUrls": true,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/index.js"
        },
        {
            "source": "/health",
            "destination": "/index.js"
        },
        {
            "source": "/",
            "destination": "/index.js"
        }
    ]
}