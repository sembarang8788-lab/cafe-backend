{
    "version": 2,
    "name": "cafe-backend",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "routes/**",
                    "models/**",
                    "config/**"
                ]
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.js"
        }
    ]
}